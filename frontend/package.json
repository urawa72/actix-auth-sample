{
  "name": "frontend",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4000",
    "build": "next build",
    "start": "next start",
    "lint": "run-s lint:*",
    "lint:eslint": "next lint --dir src",
    "lint:prettier": "prettier --check './**/*.{ts,tsx,json,css}'",
    "lint-fix": "run-p lint-fix:*",
    "lint-fix:eslint": "eslint src --fix",
    "lint-fix:prettier": "prettier --write --ignore-path .gitignore './**/*.{ts,tsx,json,css}'"
  },
  "dependencies": {
    "next": "12.0.8",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/node": "17.0.8",
    "@types/react": "17.0.38",
    "eslint": "8.7.0",
    "eslint-config-next": "12.0.8",
    "eslint-config-prettier": "8.3.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "typescript": "4.5.4"
  }
}
